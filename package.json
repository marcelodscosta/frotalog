{
  "name": "frotalog",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/server.ts src/app.ts --out-dir build",
    "start": "node build/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio"
  },
  "keywords": [],
  "author": "marcelodscosta@yahoo.com.br",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/node": "24.3.0",
    "eslint": "8.57.1",
    "prisma": "6.15.0",
    "tsx": "4.20.5"
  },
  "dependencies": {
    "@fastify/cors": "11.1.0",
    "@fastify/multipart": "9.2.1",
    "@fastify/static": "8.3.0",
    "@prisma/client": "6.15.0",
    "@types/bcryptjs": "2.4.6",
    "@types/jsonwebtoken": "9.0.10",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "bcryptjs": "3.0.2",
    "date-fns": "4.1.0",
    "dotenv": "17.2.1",
    "fastify": "5.5.0",
    "jsonwebtoken": "9.0.2",
    "multer": "2.0.2",
    "pino": "10.1.0",
    "pino-pretty": "13.1.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "vitest": "3.2.4",
    "zod": "4.1.8"
  },
  "tsup": {
    "entry": [
      "src/server.ts",
      "src/app.ts"
    ],
    "outDir": "build",
    "format": [
      "cjs"
    ],
    "clean": true,
    "external": [
      "@prisma/client",
      ".prisma/client"
    ]
  }
}
